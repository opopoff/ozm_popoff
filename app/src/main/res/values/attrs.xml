<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AspectRatioImageView">
        <attr name="widthRatio" format="float" />
        <attr name="heightRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="CompoundButton">
        <attr name="android:button" />
    </declare-styleable>

    <declare-styleable name="SlidingTabLayout">
        <attr name="slidingTab_textSize" format="dimension"/>
        <attr name="slidingTab_textColor" format="color"/>
        <attr name="slidingTab_paddingHorizontal" format="dimension"/>
        <attr name="slidingTab_paddingVertical" format="dimension"/>
    </declare-styleable>

</resources>
