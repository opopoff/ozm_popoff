<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- http://stackoverflow.com/questions/7568579/eclipsefindbugs-exclude-filter-files-doesnt-work -->
    <Match>
        <Class name="~.*\.R\$.*" />
    </Match>
    <Match>
        <Class name="~.*\.Manifest\$.*" />
    </Match>
    <Match>
        <Class name="com.ozm.fun.base.mvp.BasePresenter" />
    </Match>

    <Match>
        <Package name="io.realm" />
    </Match>

    <Match>
        <Class name="com.ozm.fun.data.FileService" />
    </Match>

</FindBugsFilter>